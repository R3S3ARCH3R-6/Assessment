package ex1;

public class Demo1 {

	public static void main(String[] args) {
		String s1 = "hello";
		String s2 = "hello";
		String s3 = new String("hello");
		
		//hashCode is the unique ID given to an address of data
		System.out.println("s1.hashCode() = " + s1.hashCode());
		System.out.println("s2.hashCode() = " + s2.hashCode());
		System.out.println("s3.hashCode() = " + s3.hashCode());
		
		//address of hash codes
		System.out.println("address of s3 = " + indexOfHashString(s3));
		System.out.println("address of s3 = " + indexOfHashString(s2));
		System.out.println("address of s3 = " + indexOfHashString(s1));
	}

}
