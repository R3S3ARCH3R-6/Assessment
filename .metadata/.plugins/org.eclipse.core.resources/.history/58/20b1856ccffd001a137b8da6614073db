package product_assignment;

public class Product {

	private int id;
	private String name;
	private int rating;
	private float cost;
	
	public Product(int id, String name, int rating, float cost){
		this.id = id;
		this.name = name;
		this.rating = rating;
		this.cost = cost;
	}
	
	public Product(int id, String name, int rating, float cost){
		this(id, name, rating, cost);
	}
	
	
}
