package ex1;

import java.util.Arrays; //get array sorting algorithm

public class array_assignment {
	
	public static void oddNum(double[] numArray){
		System.out.print("The odd numbers are: ");
		for(int i = 0; i < numArray.length; i++){
			if(numArray[i]%2 == 1){
				System.out.print(numArray[i] + " ");
			}
		}
		System.out.println("\n");
		
	}

	public static void main(String[] args) {
		
		//Odd Number
		System.out.println("Odd Number Ex:");
		int ar[]= {10,22,33,11,10,10,11,22,44,22};
		System.out.println(Arrays.toString(ar));
		oddNum(ar);
		
		//Prime Number
		int ar2[]= {1,23,33,11,10,10,11,22,47,22};
		System.out.println(Arrays.toString(ar2));
		System.out.print("The prime numbers are: ");
		for(int i = 0; i < ar2.length; i++){
			int numVal = 0;
			for(int j = 2; j <= ar2[i]; j++){
				if(i%j == 0){
					numVal++;
					
				}
			}
			if(numVal == 2){
				System.out.print(ar2[i] + " ");
			}
		}
		
		//Palindrome
		String[] palArr1 = {"wow"};
		for(int i = 0; i < palArr1.length; i++){
			
		}

	}

}
