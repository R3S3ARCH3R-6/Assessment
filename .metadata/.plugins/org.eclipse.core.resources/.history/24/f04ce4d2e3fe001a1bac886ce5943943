package regex_Ex;

public class RegexInfo {

	public static boolean ssnCheck(String s){
		boolean ans = false;
		if((s.matches("[0-9]{3}-[0-9]{2}-[0-9]{4}"))){
			ans = true;
		}
		
		return ans;
	}
	
	public static boolean licenseCheck(String s){
		boolean ans = false;
		if((s.matches("[0+0-9]{9}"))){
			ans = true;
		}
		
		return ans;
	}
	
	public static boolean areaCheck(String s){
		boolean ans = false;
		if((s.matches("[0-9]{3}"))){
			ans = true;
		}
		
		return ans;
	}
	
	public static boolean licensePlateCheck(String s){
		boolean ans = false;
		if((s.matches("[A-Z0-9]"))){
			ans = true;
		}
		
		return ans;
	}
	
	public static void main(String[] args) {
		String ssn1 = "765-21-9854";
		String ssn2 = "123-112-4522";
		String ssn3 = "176250912";
		
		boolean ssnCh1 = ssnCheck(ssn1);
		boolean ssnCh2 = ssnCheck(ssn2);
		boolean ssnCh3 = ssnCheck(ssn3);
		
 		if(ssnCh1){
			
		}

	}

}
