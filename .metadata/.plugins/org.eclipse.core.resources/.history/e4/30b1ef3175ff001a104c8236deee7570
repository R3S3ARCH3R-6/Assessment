package interface_demo;

public class Main {

	public static void main(String[] args) {
		
		//utilizing interfaces in "main"
		MyInterface1 m1 = new Implementor();
		System.out.println("Accessing using MyInterface1's reference");
		m1.common();
		m1.myMethod();
		m1.myMethod2();
		m1.myMethod3();
		
		MyInterface2 m2 = new Implementor();
		System.out.println("Access using MyInterface2's reference");
		m2.common();
		m2.method6();
		m2.method7();
	}

}
