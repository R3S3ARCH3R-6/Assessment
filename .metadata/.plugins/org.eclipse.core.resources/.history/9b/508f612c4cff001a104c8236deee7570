package ex1;

public class Player {
	private int id;
	private String name;
	private int age;
	
	public Player(){
		
	}
	
	public Player(int id, String name, int age){
		super();
		this.id = id;
		this.name = name;
		this.age = age;
	}
	
	@Override
	public String toString(){
		return "Player [id = " + id + ", name = " + name ", age = " + age;
	}
}
