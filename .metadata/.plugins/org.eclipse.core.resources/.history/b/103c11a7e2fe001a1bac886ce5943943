package regex_Ex;

public class RegexInfo {

	public static boolean ssnCheck(String s){
		boolean ans = false;
		if((s.matches("[0-9]")) && (s.length() == 9)){
			ans = true;
		}
		
		return ans;
	}
	
	public static boolean licenseCheck(String s){
		boolean ans = false;
		if((s.matches("[0+0-9]")) && (s.length() == 9)){
			ans = true;
		}
		
		return ans;
	}
	
	public boolean areaCheck(String s){
		boolean ans = false;
		if((s.matches("[0-9]")) && (s.length() == 3)){
			ans = true;
		}
		
		return ans;
	}
	
	public boolean licensePlateCheck(String s){
		boolean ans = false;
		
		
		return ans;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
