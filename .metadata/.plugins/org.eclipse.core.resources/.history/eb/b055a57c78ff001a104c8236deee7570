package functional_interfaces_lambda_demo;

public class HelloFunctionModularLambdaStyle {

	public static void main(String[] args){
		//functional programming ex
		HelloFunctional h=()->{
			System.out.println("hello lambda");
		};
		//alt version
		//HelloFunctional h=()-> System.out.println("hello lambda");
		h.hello();
		
		HelloFunctional h2=()->System.out.println("hello lambda again");
		h2.hello();
		
		
		Addition a = (x, y, z)->{return x + y + z};	//I don't need to say types, it already knows/will figure them out
	}
	
}
