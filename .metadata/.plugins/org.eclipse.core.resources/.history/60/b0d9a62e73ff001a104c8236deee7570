package abstractionclass_demo;

public class EmsMain {

	public static void main(String[] args) {

		//how we make obj of abstract classes
		EMS abcEms = new AbcEmployer();

		abcEms.createEmployee();
		abcEms.getEmployee();
		abcEms.updateProject();
		abcEms.commonEmployeeBenefits();
		abcEms.deleteEmployee();
	}

}
